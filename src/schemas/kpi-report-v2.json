{
    "title": "Telecom KPI Dashboard",
    "filters": [
        {
            "key": "dateRange",
            "type": "dateRange",
            "label": "Date Range",
            "defaultValue": "thisMonth"
        },
        {
            "key": "region",
            "type": "select",
            "label": "Region",
            "dataSource": "/api/regions",
            "placeholder": "Select Region"
        }
    ],
    "rows": [
        {
            "gutter": 16,
            "columns": [
                {
                    "span": 12,
                    "widget": {
                        "type": "chart",
                        "title": "Call Drop Rate (%)",
                        "height": 450,
                        "chartType": "line",
                        "xAxis": [
                            "Jan",
                            "Feb",
                            "Mar",
                            "Apr",
                            "May",
                            "Jun"
                        ],
                        "series": [
                            {
                                "name": "Drop Rate",
                                "data": [
                                    0.5,
                                    0.4,
                                    0.6,
                                    0.3,
                                    0.5,
                                    0.2
                                ]
                            }
                        ]
                    }
                },
                {
                    "span": 12,
                    "widget": {
                        "type": "chart",
                        "title": "Data Usage (TB)",
                        "height": 450,
                        "chartType": "bar",
                        "xAxis": [
                            "Jan",
                            "Feb",
                            "Mar",
                            "Apr",
                            "May",
                            "Jun"
                        ],
                        "series": [
                            {
                                "name": "Data Usage",
                                "data": [
                                    120,
