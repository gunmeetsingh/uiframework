{
    "title": "MCC-MNC Mappings",
    "endpoint": "/api/gtp/mccmnc-mappings",
    "permission": "gtp:mapping:manage",
    "dbPool": "GTP_PROXY",
    "tableName": "gtp_mccmnc_map",
    "metadataConfig": {
        "deleteType": "soft",
        "updatedByField": "updated_by",
        "updatedAtField": "updated_at",
        "lastActionField": "last_action",
        "listFilter": "last_action != 'D' OR last_action IS NULL"
    },
    "fields": [
        {
            "name": "nw_name",
            "label": "Network Name",
            "type": "text",
            "required": true,
            "primary": true
        },
        {
            "name": "mcc",
            "label": "MCC",
            "type": "text",
            "required": true
        },
        {
            "name": "mnc",
            "label": "MNC",
            "type": "text",
            "required": true
        },
        {
            "name": "handle_mnc",
            "label": "MNC to use for Handling",
            "type": "text",
            "required": true
        }
    ]
}