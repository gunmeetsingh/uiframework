{
    "title": "GTP IMSI Range Configuration",
    "endpoint": "/api/gtp/imsi-ranges",
    "permission": "gtp:imsi:manage",
    "dbPool": "GTP_PROXY",
    "tableName": "gtp_imsi_ranges",
    "metadataConfig": {
        "deleteType": "soft",
        "updatedByField": "updated_by",
        "updatedAtField": "updated_at",
        "lastActionField": "last_action",
        "listFilter": "last_action != 'D' OR last_action IS NULL"
    },
    "fields": [
        {
            "name": "range_name",
            "label": "IMSI Range Name",
            "type": "text",
            "required": true,
            "primary": true
        },
        {
            "name": "from_imsi",
            "label": "From IMSI",
            "type": "text",
            "required": true
        },
        {
            "name": "to_imsi",
            "label": "To IMSI",
            "type": "text",
            "required": true
        }
    ]
}